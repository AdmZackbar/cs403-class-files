class Conditional
{

}

main(args)
{
    println("Printing 1-10");
    var i = 1;
    while (i <= 10)
    {
        println(i);
        i = i + 1;
    }
    
    println("1 < 2: ", 1<2);
    println("2 < 2: ", 2<2);
    println("0 && 1: ", 0 && 1);
    println("0 || 1: ", 0 || 1);
    println("0 || 0: ", 0 || 0);
    println("1 && 4: ", 1 && 4);
    
    while (false)
    {
        print("not going to print");
    }
    
    do
    {
        print("printing once in do while");
    } while (false);
    
    while (true)
    {
        print("breaking out of infinite loop");
        return 0;
    }
}

