class Conditional
{
    function condReturn(x)
    {
        if (x)
        {
            return -1;
        }
        return 0;
    }
    
    function altWhile(len)
    {
        var i = 0;
        while (true)
        {
            if (i >= len)
            {
                return i;
            }
        }
        return "unreachable";
    }
}

main(args)
{
    println("Printing 1-10");
    var i = 1;
    while (i <= 10)
    {
        println(i);
        i = i + 1;
    }
    
    println("1 < 2: ", 1<2);
    println("2 < 2: ", 2<2);
    println("0 && 1: ", 0 && 1);
    println("0 || 1: ", 0 || 1);
    println("0 || 0: ", 0 || 0);
    println("1 && 4: ", 1 && 4);
    
    while (false)
    {
        print("not going to print");
    }
    
    do
    {
        println("printing once in do while");
    } while (false);
    
    var test = Conditional();
    
    println(test.condReturn(true), " ", test.condReturn(false));
    println(test.altWhile(10));
    
    while (true)
    {
        println("breaking out of infinite loop");
        return 0;
    }
}

