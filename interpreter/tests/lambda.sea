class LambdaTester
{
    var store;

    function multiplier(n)
    {
        return lambda(x)
        {
            return x * n;
        };
    }
}

main()
{
    var test = LambdaTester();
    
    test.store = lambda(x)
    {
        return x + 3;
    };
    
    println(test.store(2));

    var doubler = test.multiplier(2), tripler = test.multiplier(3);

    var i = 1;
    while (i < 10)
    {
        println(i, ": ", doubler(i), ", ", tripler(i));
        i = i + 1;
    }
    
    return 0;
}
